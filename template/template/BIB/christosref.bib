@InProceedings{	  kleinberg01small,
  author	= kleinberg,
  title		= "Small world phenomena and the dynamics of information",
  booktitle	= nips,
  publisher	= mitp,
  address	= mitpa,
  year		= "2001",
  annote	= " Very similar to (but developed independently of)
		  watts02identity. - If there is one group only, prob for
		  connecting two nodes is proportional to b^{-alpha.d}, where
		  b is the fanout of the tree and d is the tree distance
		  between nodes (ie., height of the first shared ancestor).
		  If outdegree of nodes if \propto log^2(n), then
		  decentralized O(log n) algo for chain letter exists {\em
		  only} for alpha=1. NOTE: this defn of alpha is different
		  from that in watts02identity. I think alpha_here * ln(b) =
		  alpha_watts - If there are multiple groups, then prob of
		  linking is proportional to the minimum distance (as in
		  watts02identity). An O(log n) decentralized search exists
		  for alpha=1, but not for alpha<1. Also exist for alpha>1. -
		  If outdegree is constant instead of polylog, then we get a
		  decentralized search algo polylog time and polylog
		  tree-leaf sizes (called resolution) only when alpha=1. This
		  appears to conflict with the watts02identity. Watts didn't
		  prove anything, but showed an experimental result with
		  alpha=1 (in their case), which isn't the same as in here.
		  The only thing I can think of is that they didn't check for
		  resolution; perhaps their resolution wasn't polylog? - I
		  couldn't understand all the math. Looks understandable
		  though; just didn't try too hard. 12/07/2003 "
}

@InProceedings{	  kleinberg02bursty,
  author	= kleinberg,
  title		= "Bursty and hierarchical structure in streams",
  booktitle	= kdd,
  publisher	= acm,
  address	= acma,
  year		= "2002",
  annote	= " Documents (such as email) arrive with different
		  inter-arrival times. How can we detect bursts (including
		  bursts within bursts etc.)? - Infinite state automaton
		  model: Each state corresponds to an exponentially
		  distributed arrival rate; the expected arrival rates are
		  increasing geometrically between the diff. states. For
		  example, state q(0) expects 10 events per sec, state q(1)
		  expects 10x events, q(2) expects 10x^2 and so on... - A
		  transition cost from q(i) -> q(j) proportional to
		  (j-i)*\gamma*ln N where N is the total number of emails.
		  However, if j<i, the transition cost is 0. - Given a set of
		  events (eg., email arrivals) along with inter-arrival
		  times, we can find the set of states which best matches the
		  data. This can be done even when we have infinite possible
		  states (but probably requires known N). - Similar model for
		  the case when we get batches of documents at regular time
		  intervals, but some of the docs are relevant, others
		  aren't. - This is modeled by having each state generate
		  some fraction of relevant to irrelevant docs... 12/12/2003
		  "
}

@InCollection{	  kleinberg05temporal,
  author	= kleinberg,
  title		= "Temporal dynamics of on-line information streams",
  booktitle	= "Data stream management: Processing high-speed data
		  streams",
  editor	= minos # and # gehrke # and # rastogi,
  publisher	= "Springer",
  year		= "2005",
  annote	= "The aim is to detect events in a stream of, say, news
		  stories: - Threshold-based models: Retrospectively detect
		  temporal episodes when the average occurance of some
		  feature exceeds its average rate by a specific threshold.
		  Issue: If the data is noisy, this might be bad. -
		  State-based methods: Low rate -> transition -> high burst
		  rate -> transition -> low rate. There can be recursive
		  levels of high burst rates, as in kleinberg02bursty. More
		  robust in presence of noise. - Trend-based models: It is an
		  event if the *trend* of the occurances is significantly
		  higher or lower than average. Thus, highest or lowest
		  slopes in the trend graph are the ones we want. + Related
		  concept: temporal term weighting: Suppose a feature (say,
		  occurance of a word) exhibits an extraordinary rising
		  trend. If a search is performed for that feature, we can
		  weigh the older documents more, on the intuition that the
		  oldest documents capture fundamental aspects of a topic
		  that later became very popular. However, this does not take
		  into account any other structure we might have, such as the
		  blog structure available in kumar03bursty. 09/09/2005 "
}

