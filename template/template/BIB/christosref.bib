@Article{	  Pennock2002,
  abstract	= {As a whole, the World Wide Web displays a striking "rich
		  get richer" behavior, with a relatively small number of
		  sites receiving a disproportionately large share of
		  hyperlink references and traffic. However, hidden in this
		  skewed global distribution, we discover a qualitatively
		  different and considerably less biased link distribution
		  among subcategories of pages[---]for example, among all
		  university homepages or all newspaper homepages. Although
		  the connectivity distribution over the entire web is close
		  to a pure power law, we find that the distribution within
		  specific categories is typically unimodal on a log scale,
		  with the location of the mode, and thus the extent of the
		  rich get richer phenomenon, varying across different
		  categories. Similar distributions occur in many other
		  naturally occurring networks, including research paper
		  citations, movie actor collaborations, and United States
		  power grid connections. A simple generative model,
		  incorporating a mixture of preferential and uniform
		  attachment, quantifies the degree to which the rich nodes
		  grow richer, and how new (and poorly connected) nodes can
		  compete. The model accurately accounts for the true
		  connectivity distributions of category-specific web pages,
		  the web as a whole, and other social networks.},
  author	= {Pennock, David M. and Flake, Gary W. and Lawrence, Steve
		  and Glover, Eric J. and Giles, Lee C.},
  citeulike-article-id={497519},
  citeulike-linkout-0={http://www.pnas.org/cgi/content/abstract/99/8/5207},
  citeulike-linkout-1={http://view.ncbi.nlm.nih.gov/pubmed/11959968},
  citeulike-linkout-2={http://www.hubmed.org/display.cgi?uids=11959968},
  day		= {16},
  journal	= {PNAS},
  month		= {April},
  number	= {8},
  pages		= {5207--5211},
  posted-at	= {2009-03-18 20:02:42},
  priority	= {2},
  title		= {Winners don't take all: Characterizing the competition for
		  links on the web},
  url		= {http://www.pnas.org/cgi/content/abstract/99/8/5207},
  volume	= {99},
  year		= {2002}
}

