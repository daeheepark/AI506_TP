@Article{	  albert00internets,
  author	= albert # and # jeong # and # barabasi,
  title		= "Error and attack tolerance of complex networks",
  journal	= nature,
  year		= "2000",
  volume	= "406",
  pages		= "378--381",
  annote	= " Authors consider two types of networks: 1) exponential
		  networks, formed by the Erdos-Renyi(ER) models, where the
		  probability distribution of degree P(k) peaks at some
		  average <k>, and decays exponentially for large k, and 2)
		  scale-free networks, where P(k) = c.k^(-gamma). The
		  observations are: - As nodes are deleted (due to errors),
		  the distance between 2 nodes increases for exponential
		  networks, but doesnt appreciably increase for scale-free
		  ones, when the fraction of deleted nodes is small. - If a
		  malicious attacker wants to take down a network, it is much
		  easier to kill a scale-free network, by simply deleting the
		  most connected nodes one after another. 12/15/02 "
}

@Article{	  albert00topology,
  author	= albert # and # barabasi,
  title		= "Topology of evolving networks: local events and
		  universality",
  journal	= prl,
  year		= "2000",
  volume	= "85",
  number	= "24",
  pages		= "5234--5237",
  annote	= " Describes a mechanism for graph generation so that
		  scale-free graphs are generated over a certain param range,
		  and exponentially decaying networks outside it. Authors
		  apply it to the movie-actor dataset. Mechanism is: at each
		  step - Add m new links by selecting some node, and
		  attaching links to neighbors with probability proportional
		  to the number of links it already has, a la preferential
		  attachment. - Rewire links: choose some node, and rewire
		  some links coming into that node; again use preferential
		  attachment for the new ends of these links. - Add new node,
		  and give it m links by preferential attachment. 12/15/02 "
}

